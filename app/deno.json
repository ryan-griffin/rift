{
	"nodeModulesDir": "auto",
	"tasks": {
		"dev": "vinxi dev",
		"build": "vinxi build",
		"start": "deno run --allow-env --allow-read --allow-net --env-file npm:vinxi start",
		"tauri": "tauri"
	},
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "solid-js",
		"lib": ["DOM", "DOM.Iterable", "ESNext"]
	},
	"fmt": {
		"useTabs": true
	}
}

{
	"nodeModulesDir": "auto",
	"tasks": {
		"dev": "deno run -A --env-file set-env.ts dev",
		"build": "deno run -A --env-file set-env.ts build",
		"start": "deno run -A --env-file set-env.ts start",
		"tauri": "tauri"
	},
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "solid-js",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext",
			"deno.ns"
		]
	},
	"fmt": {
		"useTabs": true
	}
}
